<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.generated_package">
  		<package
			uri="http://reuse/actions/miner"
			class="minerv1.impl.Minerv1PackageImpl"/>
	</extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated miner -->
      <factory
            uri="http://prisma.cos.ufrj.br/reuseactionsminer"
            class="br.ufrj.cos.prisma.model.miner.provider.Minerv1ItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated miner -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            class="minerv1.presentation.Minerv1ModelWizard"
            icon="icons/full/obj16/Minerv1ModelFile.gif"
            id="br.ufrj.cos.prisma.model.miner.presentation.Minerv1ModelWizardID"
            name="Miner1 Model">
            <description>%_UI_MinerModelWizard_description</description>
         	<selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

	<extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="ReuseActionsMiner.contribution1">
         <menu
               label="Reuse Actions Miner"
               path="additions"
               id="ReuseActionsMiner.menu1">
            <separator
                  name="group1">
            </separator>
			<separator
                  name="group2">
            </separator>
			<separator
                  name="group3">
            </separator>            
         </menu>
         <action
               label="List repositories"
               class="br.ufrj.cos.prisma.miner.popup.actions.ListRepositoriesAction"
               menubarPath="ReuseActionsMiner.menu1/group1"
               enablesFor="1"
               id="Miner.popup.actions.ListRepositoriesForExtraction">
         </action>
         <action
               label="Parse Framework"
               class="br.ufrj.cos.prisma.miner.popup.actions.ParseFrameworkAction"
               menubarPath="ReuseActionsMiner.menu1/group2"
               enablesFor="1"
               id="Miner.popup.actions.NewParser">
         </action>
		 <!--action
               label="Mine Workspace"
               class="br.ufrj.cos.prisma.miner.popup.actions.MineWorkspaceAction"
               menubarPath="ReuseActionsMiner.menu1/group1"
               enablesFor="1"
               id="Miner.popup.actions.MineWorkspaceAction">
         </action-->
         <action
               label="Mine Repositories from List"
               class="br.ufrj.cos.prisma.miner.popup.actions.MineRepositoriesActionv2"
               menubarPath="ReuseActionsMiner.menu1/group2"
               enablesFor="1"
               id="Miner.popup.actions.MineRepositoriesActionv2">
         </action>         
         <action
               label="Generate XES Log"
               class="br.ufrj.cos.prisma.miner.popup.actions.GenerateXESLogAction"
               menubarPath="ReuseActionsMiner.menu1/group3"
               enablesFor="1"
               id="Miner.popup.actions.GenerateXESLogAction">
         </action>
         
      </objectContribution>
   </extension>
 <extension
       id="ReuseActionsMinerID"
       point="org.eclipse.core.runtime.products">
    <product
          application="org.eclipse.ui.ide.workbench"
          name="ReuseActionsMiner">
       <property
             name="appName"
             value="ReuseActionsMiner">
       </property>
       <property
             name="preferenceCustomization"
             value="plugin_customization.ini">
       </property>
    </product>
 </extension>
 <extension
       id="product"
       point="org.eclipse.core.runtime.products">
    <product
          application="org.eclipse.e4.ui.workbench.swt.E4Application"
          name="ReuseActionsMiner">
       <property
             name="appName"
             value="ReuseActionsMiner">
       </property>
    </product>
 </extension>
null
</plugin>
